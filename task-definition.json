{
    "containerDefinitions": [
      {
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        },
        "portMappings": [
          {
            "hostPort": 80,
            "protocol": "tcp",
            "containerPort": 80
          },
          {
            "protocol": "tcp",
            "containerPort": 443,
            "hostPort": 443
          },
          {
            "protocol": "tcp",
            "containerPort": 3000,
            "hostPort": 3000
          }
        ],
        "cpu": 0,
        "image": "323230034331.dkr.ecr.us-east-1.amazonaws.com/talabatmenu:latest",
        "essential": true,
        "name": "TalabatMenuDev"
      }
    ],
    "memory": "2048",
    "family": "TalabatMenuDev",
    "taskRoleArn": "arn:aws:iam::323230034331:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::323230034331:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024"
  }
  